{
    "bracketSameLine": true,
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "es5",
    "arrowParens": "always",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "vueIndentScriptAndStyle": false,
    "singleQuote": true,
    "semi": true,
    "useTabs": false,
    "importOrder": [
        "^/(.*)$$",
        "^[./]",
        "^assets/(.*)$",
        "^components/(.*)$",
        "^configs/(.*)$",
        "^constants/(.*)$",
        "^contexts/(.*)$",
        "^hooks/(.*)$",
        "^i18n/(.*)$",
        "^modules/(.*)$",
        "^navigation/(.*)$",
        "^screens/(.*)$",
        "^services/(.*)$",
        "^states/(.*)$",
        "^themes$",
        "^themes/(.*)$",
        "^typings/(.*)$",
        "^utils/(.*)$",

        "^assets$",
        "^components$",
        "^configs$",
        "^constants$",
        "^contexts$",
        "^hooks$",
        "^i18n$",
        "^modules$",
        "^navigation$",
        "^screens$",
        "^services$",
        "^states$",
        "^typings$",
        "^utils$",
        "^$"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": false,
    "importOrderGroupNamespaceSpecifiers": false
}
